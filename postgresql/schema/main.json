{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schema.corezoid.com/packages/postgresql/main.json",
  "type": "object",
  "required": [ "postgresql" ],
  "properties": {
    "postgresql": {
      "type": "object",
      "default": {
        "main": {
          "host": null,
          "port": 5432,
          "username": "postgres",
          "password": ""
        }
      },
      "patternProperties": {
        ".+": { "$ref": "https://schema.corezoid.com/packages/postgresql/connection.json" }
      }
    }
  }
}
